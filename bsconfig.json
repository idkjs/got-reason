{
  "name": "parcel-reason-react-app",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "bs-dependencies": [
    "reason-react",
    "reason-apollo"
  ],
  "bsc-flags": [
    "-bs-super-errors"
  ],
  "refmt": 3,
  "reason": {
    "react-jsx": 3
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "ppx-flags": [
    "graphql_ppx/ppx",
    "get_in_ppx/ppx"
  ]
}
